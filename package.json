{
	"name": "dhalsim",
	"version": "0.1.0",
	"description": "Browser automation for llmist agents using Camoufox anti-detect browser",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"llmist": {
		"gadgets": "./dist/index.js",
		"factory": "./dist/index.js",
		"subagents": {
			"BrowseWeb": {
				"entryPoint": "./dist/index.js",
				"export": "Dhalsim",
				"description": "Browse websites and accomplish tasks autonomously",
				"uses": ["Navigate", "Click", "Screenshot", "GetFullPageContent", "Fill"],
				"defaultModel": "sonnet",
				"maxIterations": 15
			}
		},
		"presets": {
			"all": "*",
			"subagent": ["BrowseWeb"],
			"readonly": ["Navigate", "Screenshot", "GetFullPageContent", "ListPages"],
			"minimal": ["Navigate", "Screenshot", "GetFullPageContent"]
		},
		"session": {
			"factory": "getSessionManager",
			"type": "browser"
		}
	},
	"scripts": {
		"build": "bun build src/index.ts --outdir dist --target node --external playwright-core --external camoufox-js --external chromium-bidi --external electron && bun run build:types",
		"build:types": "tsc --emitDeclarationOnly",
		"typecheck": "tsc --noEmit",
		"lint": "biome lint .",
		"format": "biome format --write .",
		"check": "biome check --write .",
		"test": "vitest run --no-file-parallelism",
		"precheck": "bun run lint && bun run typecheck && bun run test",
		"prepare": "lefthook install",
		"postinstall": "rm -rf node_modules/camoufox-js/node_modules/better-sqlite3 2>/dev/null || true"
	},
	"keywords": [
		"browser",
		"automation",
		"playwright",
		"llm",
		"agent",
		"web",
		"scraping"
	],
	"author": "Zbigniew Sobiecki",
	"license": "MIT",
	"dependencies": {
		"better-sqlite3": "nounder/bun-better-sqlite3",
		"camoufox-js": "github:zbigniewsobiecki/camoufox-js#impit-fallback",
		"llmist": "^4.0.0",
		"sharp": "^0.34.5"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.2",
		"@types/node": "^20.12.7",
		"bun-types": "^1.3.2",
		"lefthook": "^1.6.0",
		"typescript": "^5.4.5",
		"vitest": "^4.0.15"
	}
}
