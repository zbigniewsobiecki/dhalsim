{
	"name": "dhalsim",
	"version": "2.2.1",
	"description": "Browser automation for llmist agents using Camoufox anti-detect browser",
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"llmist": {
		"gadgets": "./dist/index.js",
		"factory": "./dist/index.js",
		"subagents": {
			"BrowseWeb": {
				"entryPoint": "./dist/index.js",
				"export": "Dhalsim",
				"description": "Browse websites and accomplish tasks autonomously",
				"uses": [
					"Navigate",
					"Click",
					"Screenshot",
					"GetFullPageContent",
					"Fill"
				],
				"defaultModel": "sonnet",
				"maxIterations": 15
			}
		},
		"presets": {
			"all": "*",
			"subagent": [
				"BrowseWeb"
			],
			"readonly": [
				"Navigate",
				"Screenshot",
				"GetFullPageContent",
				"ListPages"
			],
			"minimal": [
				"Navigate",
				"Screenshot",
				"GetFullPageContent"
			]
		},
		"session": {
			"factory": "getSessionManager",
			"type": "browser"
		}
	},
	"scripts": {
		"build": "tsup",
		"typecheck": "tsc --noEmit",
		"lint": "biome lint .",
		"format": "biome format --write .",
		"check": "biome check --write .",
		"test": "vitest run --no-file-parallelism",
		"precheck": "npm run lint && npm run typecheck && npm run test",
		"prepare": "node -e \"process.cwd().includes('node_modules') || process.env.CI || require('child_process').execSync('npx lefthook install', {stdio: 'inherit'})\"",
		"postinstall": "node -e \"process.cwd().includes('node_modules') || process.env.CI || require('child_process').execSync('npx lefthook install', {stdio: 'inherit'})\""
	},
	"keywords": [
		"browser",
		"automation",
		"playwright",
		"llm",
		"agent",
		"web",
		"scraping"
	],
	"author": "Zbigniew Sobiecki",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/zbigniewsobiecki/dhalsim.git"
	},
	"homepage": "https://github.com/zbigniewsobiecki/dhalsim#readme",
	"bugs": {
		"url": "https://github.com/zbigniewsobiecki/dhalsim/issues"
	},
	"files": [
		"dist",
		"dhalsim-icon.png",
		"LICENSE",
		"README.md"
	],
	"dependencies": {
		"camoufox-js": "github:zbigniewsobiecki/camoufox-js#86d1dab",
		"playwright-core": "^1.57.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.2",
		"@commitlint/cli": "^20.1.0",
		"@commitlint/config-conventional": "^20.0.0",
		"@llmist/testing": "^12.0.0",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^12.0.2",
		"@semantic-release/npm": "^13.1.3",
		"@types/node": "^20.12.7",
		"conventional-changelog-conventionalcommits": "^9.1.0",
		"lefthook": "^1.6.0",
		"llmist": "^12.0.0",
		"semantic-release": "^25.0.2",
		"tsup": "^8.5.0",
		"typescript": "^5.4.5",
		"vitest": "^4.0.15"
	},
	"peerDependencies": {
		"llmist": ">=12.0.0"
	},
	"engines": {
		"node": ">=22.0.0"
	}
}
